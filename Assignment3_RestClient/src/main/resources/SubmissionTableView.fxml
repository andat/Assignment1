<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.SubmissionController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="submissionTable" editable="true" prefHeight="369.0" prefWidth="580.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="nameCol" prefWidth="75.0" text="Assignment name">
                    <cellValueFactory>
                        <PropertyValueFactory property="assignmentName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="userCol" prefWidth="75.0" text="Student username">
                    <cellValueFactory>
                        <PropertyValueFactory property="studentUsername" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="gradeCol" prefWidth="75.0" text="Grade">
                    <cellValueFactory>
                        <PropertyValueFactory property="grade" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="dateCol" prefWidth="75.0" text="Date">
                    <cellValueFactory>
                        <PropertyValueFactory property="date" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="descriptionCol" prefWidth="75.0" text="Description">
                    <cellValueFactory>
                        <PropertyValueFactory property="description" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </TableView>
        <HBox spacing="10.0">
            <children>
                <Button fx:id="viewBtn" mnemonicParsing="false" onAction="#viewBtnClicked" text="View all">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Button>
                <Button fx:id="editBtn" alignment="CENTER" mnemonicParsing="false" onAction="#editBtnClicked" text="Edit">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Button>
                <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#deleteBtnClicked" text="Delete">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Button>
                <Button fx:id="gradeBtn" mnemonicParsing="false" onAction="#deleteBtnClicked" text="Grade submission">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="30.0" right="30.0" top="10.0" />
            </VBox.margin>
        </HBox>
